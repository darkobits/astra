{
  "name": "@darkobits/astra",
  "version": "0.1.4",
  "author": "darkobits",
  "description": "A VS Code theme.",
  "license": "WTFPL",
  "repository": {
    "type": "git",
    "url": "https://github.com/darkobits/astra"
  },
  "files": [
    "dist",
    "themes",
    ".vsctrc"
  ],
  "scripts": {
    "clean": "del dist && del themes",
    "check:styles": "tslint --project tsconfig.json --format codeFrame",
    "check:types": "tsc --noEmit",
    "prebuild": "run-p clean check:*",
    "build": "babel src --out-dir dist --extensions '.ts' --verbose",
    "postbuild": "vsct compile",
    "bump": "run-s build && standard-version",
    "bump:beta": "npm run bump -- --prerelease=beta",
    "start": "concurrently 'npm run build -- --watch' 'vsct start'",
    "prepare": "run-s build postinstall",
    "postinstall": "vsct install"
  },
  "dependencies": {
    "@darkobits/vsct": "^0.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.0",
    "@babel/core": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-typescript": "^7.1.0",
    "@types/node": "^10.10.1",
    "babel-plugin-module-resolver": "^3.1.1",
    "concurrently": "^4.0.1",
    "del-cli": "^1.1.0",
    "npm-run-all": "^4.1.3",
    "standard-version": "^4.4.0",
    "tslint": "^5.11.0",
    "tslint-xo": "^0.9.0",
    "typescript": "^3.0.3"
  }
}
