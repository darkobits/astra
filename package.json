{
  "name": "@darkobits/astra",
  "version": "0.1.3",
  "author": "darkobits",
  "publisher": "darkobits",
  "description": "A VS Code theme.",
  "license": "WTFPL",
  "repository": {
    "type": "git",
    "url": "https://github.com/darkobits/astra"
  },
  "files": [
    "themes"
  ],
  "scripts": {
    "clean": "del dist && del themes",
    "check:styles": "tslint --project tsconfig.json --format codeFrame",
    "check:types": "tsc --noEmit",
    "prebuild": "run-p clean check:*",
    "build": "babel src --out-dir dist --extensions '.ts' --verbose",
    "prepare": "npm run build && vsct build && vsct install",
    "bump": "run-s build && standard-version",
    "bump:beta": "npm run bump -- --prerelease=beta",
    "start": "concurrently \"npm run build -- --watch\" \"vsct start\"",
    "postinstall": "vsct install"
  },
  "dependencies": {
    "@darkobits/vsct": "^0.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-rc.3",
    "@babel/core": "^7.0.0-rc.3",
    "@babel/node": "^7.0.0-rc.3",
    "@babel/preset-env": "^7.0.0-rc.3",
    "@babel/preset-typescript": "^7.0.0-rc.3",
    "@babel/register": "^7.0.0-rc.3",
    "@types/color": "^3.0.0",
    "@types/fs-extra": "^5.0.4",
    "@types/node": "^10.9.2",
    "babel-plugin-module-resolver": "^3.1.1",
    "color": "^3.0.0",
    "concurrently": "^4.0.1",
    "del-cli": "^1.1.0",
    "npm-run-all": "^4.1.3",
    "standard-version": "^4.4.0",
    "tslint": "^5.11.0",
    "tslint-xo": "^0.9.0",
    "typescript": "^3.0.1"
  }
}
