{
  "presets": [
    ["@babel/preset-env", {
      "targets": {
        "node": "8.0.0"
      }
    }],
    "@babel/preset-typescript"
  ],
  "plugins": [
    ["babel-plugin-module-resolver", {
      "cwd": "babelrc",
      "root": ["./src"],
      "extensions": [
        ".ts",
        ".tsx",
        ".js",
        ".jsx",
        ".json"
      ]
    }]
  ]
}
